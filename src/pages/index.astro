---
import MainPage from "../components/MainPage.astro";
import MainTest from "../components/MainTest.astro";
import Layout from "../layouts/Layout.astro";
import { fade } from "astro:transitions";
import { ViewTransitions } from "astro:transitions";
const title = "Toadland - Приватный майнкрафт сервер";
const description =
  "Приватный minecraft сервер 1.20.1 с модами и дружелюбной атмосферой!";
---

<ViewTransitions />
<Layout title={title} description={description}>
  <main>
    <MainTest slot="MainPage2" />
  </main>
</Layout>

<style>
  body {
    margin: 0;
    padding: 0;
  }
</style>
